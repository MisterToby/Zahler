<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zahler\ZahlerBundle\Entity\Loan" table="loan">
    <indexes>
      <index name="IDX_C5D30D0360917A4F" columns="loa_per_id"/>
      <index name="IDX_C5D30D036B6C8BBA" columns="loa_tra_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="loaDescription" type="string" column="loa_description" length="255" nullable="true"/>
    <field name="loaInterestRate" type="float" column="loa_interest_rate" precision="10" scale="0" nullable="false"/>
    <many-to-one field="loaTra" target-entity="Transaction">
      <join-columns>
        <join-column name="loa_tra_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="loaPer" target-entity="Person">
      <join-columns>
        <join-column name="loa_per_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
